{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://mdstudio/schemas/adan_dene_request.json",
    "title": "Adan dene input",
    "description": "eTOX ALLIES deltaG VdW/Elec energy applicability domain analysis",
    "type": "object",
    "properties": {
	"dataframe":{
	    "type": "string",
	    "default": "energy decomposition dataframe"
	},
	"model_pkl": {
	    "type": "string",
	    "description": "Pickle file containing the model"
	},
	"center": {
	    "type": ["null", "array"],
	    "default": "null"
	},
	"ci_cutoff": {
	    "type": ["number", "null"],
	    "default": "null"
	},
	"workdir": {
	    "type": "string",
	    "default": "."
	}
    },
    "required": ["dataframe", "model_pka"]
}
